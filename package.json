{
  "homepage": "http://Ihsas01.github.io/Wedding-Hall-Booking-System",
  "name": "wedding-hall-booking",
  "version": "1.0.0",
  "description": "A full-stack wedding hall booking system",
  "main": "index.js",
  "scripts": {
    "install-all": "npm install && cd client && npm install && cd ../server && npm install",
    "client": "cd client && npm run dev",
    "server": "cd server && npm run dev",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d client/dist",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client && npm run build",
    "start": "cd server && npm start"
  },
  "keywords": [
    "wedding",
    "booking",
    "hall",
    "react",
    "node",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "gh-pages": "^6.3.0"
  }
}
